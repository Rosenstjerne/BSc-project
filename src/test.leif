var int[][] A;
var int size;
var int[] sizes;

function null fill(int[] arr, int s) {
  var int i;

  i = 0;
  while (i < s) {
    arr[i] = i * 2;
    i = i + 1;
  }
}

function null show(int[] arr, int s) {
  var int i;

  i = 0;
  while (i < s) {
    print(arr[i]);
    i = i + 1;
  }
}

size = 5;
A = new int[][size];
sizes = new int[size];

A[0] = new int[5];
sizes[0] = 5;
fill(A[0], sizes[0]);

A[1] = new int[10];
sizes[1] = 10;
fill(A[1], sizes[1]);

A[2] = new int[2];
sizes[2] = 2;
fill(A[2], sizes[2]);

A[3] = new int[17];
sizes[3] = 17;
fill(A[3], sizes[3]);

A[4] = new int[0];
sizes[4] = 0;
fill(A[4], sizes[4]);

show(A[0], sizes[0]);
print(true);
show(A[1], sizes[1]);
print(true);
show(A[2], sizes[2]);
print(true);
show(A[3], sizes[3]);
print(true);
show(A[4], sizes[4]);
print(true);
